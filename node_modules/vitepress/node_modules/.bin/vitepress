#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/vue/dev-doc/node_modules/.pnpm/vitepress@1.3.2_@algolia+client-search@4.24.0_search-insights@2.16.0/node_modules/vitepress/bin/node_modules:/mnt/d/vue/dev-doc/node_modules/.pnpm/vitepress@1.3.2_@algolia+client-search@4.24.0_search-insights@2.16.0/node_modules/vitepress/node_modules:/mnt/d/vue/dev-doc/node_modules/.pnpm/vitepress@1.3.2_@algolia+client-search@4.24.0_search-insights@2.16.0/node_modules:/mnt/d/vue/dev-doc/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/vue/dev-doc/node_modules/.pnpm/vitepress@1.3.2_@algolia+client-search@4.24.0_search-insights@2.16.0/node_modules/vitepress/bin/node_modules:/mnt/d/vue/dev-doc/node_modules/.pnpm/vitepress@1.3.2_@algolia+client-search@4.24.0_search-insights@2.16.0/node_modules/vitepress/node_modules:/mnt/d/vue/dev-doc/node_modules/.pnpm/vitepress@1.3.2_@algolia+client-search@4.24.0_search-insights@2.16.0/node_modules:/mnt/d/vue/dev-doc/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/vitepress.js" "$@"
else
  exec node  "$basedir/../../bin/vitepress.js" "$@"
fi
